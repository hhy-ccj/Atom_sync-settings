{
	"*": {
		"activate-power-mode": {
			"plugins": {
				"playAudio": true,
				"screenShake": true
			}
		},
		"core": {
			"autoHideMenuBar": true,
			"closeDeletedFileTabs": true,
			"disabledPackages": [
				"atom-ide-ui",
				"busy-signal",
				"intentions",
				"language-gfm",
				"markdown-preview",
				"sync-settings-folder-location",
				"gcc-make-run",
				"simplified-chinese-menu"
			],
			"openEmptyEditorOnStart": false,
			"restorePreviousWindowsOnStart": "no",
			"telemetryConsent": "no",
			"themes": [
				"dracula-ui",
				"dracula-syntax"
			],
			"uriHandlerRegistration": "always"
		},
		"dev-tools-themes": {
			"theme": "nightlion"
		},
		"editor": {
			"invisibles": {},
			"preferredLineLength": 500,
			"scrollPastEnd": true,
			"showIndentGuide": true,
			"softWrap": true
		},
		"exception-reporting": {
			"userId": "0de6819b-4432-47c1-a890-45133950981b"
		},
		"gcc-make-run": {},
		"github": {
			"viewChangesForCurrentFileDiffPaneSplitDirection": "down"
		},
		"gtk-dark-theme": {},
		"highlight-selected": {
			"showResultsOnScrollBar": true
		},
		"language-markdown": {},
		"linter": {},
		"linter-gcc": {
			"compileCommandsFile": "./compile_commands.json",
			"execPath": "gcc",
			"gccDebug": true,
			"gccLintOnTheFly": true,
			"gccLintOnTheFlyInterval": 1000,
			"gccRelintMessageSources": true
		},
		"linter-ui-default": {
			"panelHeight": 157.33334350585938
		},
		"markdown-preview": {
			"useGitHubStyle": true
		},
		"markdown-preview-plus": {
			"syntaxThemeName": "one-light-syntax",
			"useGitHubStyle": true
		},
		"minimap": {
			"displayMinimapOnLeft": true,
			"plugins": {
				"highlight-selected": true,
				"highlight-selectedDecorationsZIndex": 0,
				"minimap-quick-highlight": true,
				"minimap-quick-highlightDecorationsZIndex": 0
			}
		},
		"sync-settings": {
			"hiddenSettings": {},
			"useOtherLocation": true
		},
		"sync-settings-folder-location": {
			"folderPath": "D:\\hhy\\Atom_sync_local"
		},
		"sync-settings-git-location": {
			"gitUrl": "https://github.com/hhy-ccj/Atom_sync-settings"
		},
		"welcome": {
			"showOnStartup": false
		}
	}
}