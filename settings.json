{
	"*": {
		"activate-power-mode": {
			"plugins": {
				"playAudio": true,
				"screenShake": true
			}
		},
		"atom-clock": {
			"dateFormat": "[Quarter] Q, MMMM Do YYYY, ddd, h:mm a",
			"locale": "zh_CN",
			"showClockIcon": true,
			"showTooltip": true
		},
		"atom-cscope": {
			"EnableHistory": true,
			"MaxCscopeResults": 100,
			"WidgetLocation": "bottom"
		},
		"atom-ctags": {
			"autoBuildTagsWhenActive": true
		},
		"core": {
			"autoHideMenuBar": true,
			"closeDeletedFileTabs": true,
			"disabledPackages": [
				"atom-ide-ui",
				"busy-signal",
				"intentions",
				"language-gfm",
				"markdown-preview",
				"sync-settings-folder-location",
				"gcc-make-run",
				"simplified-chinese-menu",
				"linter",
				"linter-gcc",
				"linter-ui-default",
				"symbols-view"
			],
			"openEmptyEditorOnStart": false,
			"restorePreviousWindowsOnStart": "no",
			"telemetryConsent": "no",
			"themes": [
				"steam-pirate-ui",
				"steam-pirate-syntax"
			],
			"uriHandlerRegistration": "always"
		},
		"dev-tools-themes": {
			"theme": "nightlion"
		},
		"editor": {
			"invisibles": {},
			"preferredLineLength": 500,
			"scrollPastEnd": true,
			"showIndentGuide": true,
			"softWrap": true
		},
		"ex-mode": {},
		"exception-reporting": {
			"userId": "0de6819b-4432-47c1-a890-45133950981b"
		},
		"gcc-make-run": {},
		"github": {
			"viewChangesForCurrentFileDiffPaneSplitDirection": "down"
		},
		"glowing-cursor": {
			"cursorWidth": 10,
			"glowDistance": 8
		},
		"goto-definition": {
			"contextMenuDisplayAtFirst": false
		},
		"gtk-dark-theme": {},
		"highlight-selected": {
			"showResultsOnScrollBar": true
		},
		"language-markdown": {},
		"linter": {},
		"linter-gcc": {
			"compileCommandsFile": "./compile_commands.json",
			"execPath": "gcc",
			"gccDebug": true,
			"gccLintOnTheFly": true,
			"gccLintOnTheFlyInterval": 1000,
			"gccRelintMessageSources": true
		},
		"linter-ui-default": {
			"panelHeight": 157.33334350585938,
			"showPanel": true
		},
		"markdown-preview": {
			"useGitHubStyle": true
		},
		"markdown-preview-plus": {
			"syntaxThemeName": "one-light-syntax",
			"useGitHubStyle": true
		},
		"minimap": {
			"plugins": {
				"highlight-selected": true,
				"highlight-selectedDecorationsZIndex": 0,
				"minimap-quick-highlight": true,
				"minimap-quick-highlightDecorationsZIndex": 0
			}
		},
		"relative-numbers": {
			"showAbsoluteNumbers": true,
			"softWrapsCount": false
		},
		"steam-pirate-ui": {
			"backgroundImage": "http://img.3dmgame.com/uploads/allimg/170710/316-1FG0140Q7.jpg",
			"blurBackground": "1"
		},
		"sync-settings": {
			"checkForUpdatedBackup": false,
			"hiddenSettings": {},
			"useOtherLocation": true
		},
		"sync-settings-folder-location": {
			"folderPath": "D:\\hhy\\Atom_sync_local"
		},
		"sync-settings-git-location": {
			"gitUrl": "https://github.com/hhy-ccj/Atom_sync-settings"
		},
		"welcome": {
			"showOnStartup": false
		}
	}
